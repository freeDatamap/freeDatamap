                "execBeforeAll": "
if(!node.children) {
    node.rawNode['items.dm_props'].visible = false ;
    node.rawNode['items.dm_props'].dontExport = true ;
    node.rawNode['level1cloud.dm_props'].visible = false ;
    node.rawNode['wikipediaDescription.dm_props'].delayDisplay = 0 ;
} else if(dm_Tools.getChildrenAtLevel(node).length <= 5) {
    node.rawNode['level1cloud.dm_props'].visible = false ;
}
if(node.rawNode.url) {
    if(((''+node.rawNode.url).indexOf('http:/') == 0) || ((''+node.rawNode.url).indexOf('https:/') == 0)) {
        if((''+node.rawNode.url).indexOf('wikipedia') > 0) {
            var urlElts = (''+node.rawNode.url).split('/') ;
            var wikipediaKey = urlElts[urlElts.length-1] ;
            wikipediaKey = wikipediaKey.replace(/_/g, '+') ;
            node.rawNode.wikipediaDescription = '<embed src=\"http://en.wikipedia.org/w/index.php?title=' + wikipediaKey + '&amp;printable=yes\" style=\"width:100%; min-height:450px; height:95%;\"></embed>' ;
        } else {
            node.rawNode.wikipediaDescription = '<embed src=\"' + node.rawNode.url + '\" style=\"width:100%; min-height:450px; height:95%;\"></embed>' ;
        }
    } else if((''+node.rawNode.url).indexOf('.pdf') == (''+node.rawNode.url).length - '.pdf'.length) {
        if((''+node.rawNode.url).indexOf('http://www.datascienceontology.com/') != 0) {
            node.rawNode.url = 'http://www.datascienceontology.com/' + node.rawNode.url ;
            //node.rawNode['url.dm_props']['content.url'] = node.rawNode.url ;
        }
        node.rawNode.wikipediaDescription = '<embed src=\"' + node.rawNode.url + '?q=get%20wiki%20data\" style=\"width:100%; min-height:450px; height:95%;\"></embed>' ;
    }
}
'#dm_STOP_AND_REDRAW_ATTRIBUTES_PANEL#1' ;
